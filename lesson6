      "cell_type": "code",
      "source": [
        "def title_organizer(title):\n",
        "    # Example implementation: Clean and standardize the title\n",
        "    title = title.strip()  # Remove leading and trailing spaces\n",
        "    title = title.replace('(', '( ').replace(')', ' )')  # Adding space after '(' and before ')'\n",
        "    return title\n",
        "\n",
        "# Test the function\n",
        "formatted_title = title_organizer('Business Intelligence Analyst Senior(Chief Financial Office)')\n",
        "print(formatted_title)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
